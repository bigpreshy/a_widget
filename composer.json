{
    "name": "acme-widget-co/basket-system",
    "description": "Acme Widget Co Shopping Basket System",
    "type": "project",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "AcmeWidgetCo\\": "src/"
        },
        "files": [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AcmeWidgetCo\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyze": "vendor/bin/phpstan analyse src tests --level max"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },

    "authors": [
        {
            "name": "Precious Luke"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}